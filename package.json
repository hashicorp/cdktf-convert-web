{
  "name": "convert-page",
  "version": "1.0.0",
  "main": "main.js",
  "types": "main.ts",
  "license": "MPL-2.0",
  "private": true,
  "scripts": {
    "postinstall": "cd frontend && npm install --force",
    "frontend": "cd frontend && REACT_APP_BACKEND_URL=$(cat ../outputs.json | jq -r '.development[\"convert-backend-url\"]') npm start",
    "get": "cdktf get",
    "build": "tsc",
    "synth": "cdktf synth",
    "compile": "tsc --pretty",
    "watch": "tsc -w",
    "predeploy": "cd frontend && NODE_ENV=production npm run build",
    "deploy": "cdktf deploy '*' --auto-approve --outputs-file=outputs.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "upgrade": "npm i cdktf@latest cdktf-cli@latest",
    "upgrade:next": "npm i cdktf@next cdktf-cli@next"
  },
  "engines": {
    "node": ">=10.12"
  },
  "dependencies": {
    "@cdktf-plus/aws": "^0.3.0",
    "@cdktf/provider-aws": "^7.0.13",
    "@cdktf/provider-external": "^0.5.12",
    "@cdktf/provider-null": "^0.6.13",
    "@types/mime-types": "^2.1.1",
    "cdktf": "^0.10.1",
    "cdktf-local-build": "^0.0.81",
    "cdktf-local-exec": "^0.0.93",
    "constructs": "^10.0.111",
    "glob": "^8.0.1",
    "iam-floyd": "^0.286.0",
    "mime": "^3.0.0",
    "mime-types": "^2.1.35"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/jest": "^27.4.0",
    "@types/mime": "^2.0.3",
    "@types/node": "^17.0.8",
    "jest": "^27.4.7",
    "ts-jest": "^27.1.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  }
}
